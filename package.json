{
    "name": "yaml-as-json-editor",
    "version": "1.0.0",
    "description": "",
    "main": "./out/extension.js",
    "author": "",
    "license": "ISC",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/node": "^16.18.34",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.26.0",
        "monaco-editor": "^0.47.0",
        "typescript": "^5.4.2",
        "yaml": "^2.4.2"
    },
    "engines": {
        "vscode": "^1.74.0"
    },
    "contributes": {
        "customEditors": [
            {
                "viewType": "eh.yamlAndJson",
                "displayName": "Yaml and Json cross editor",
                "selector": [
                    {
                        "filenamePattern": "*.yaml"
                    }
                ]
            }
        ],
        "commands": []
    }
}
